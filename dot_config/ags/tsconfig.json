{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "lib": [
            "ES2022"
        ],
        "allowJs": true,
        "baseUrl": ".",
        "typeRoots": [
            "./node_modules/@girs",
            "./ags.ts",
            "./types",
            "./node_modules/duolingo-api-js"
        ],
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "config.ts",
        "ags.ts",
        "@girs",
        "duolingo-api-js",
        "types",
        "js"
    ]
}